<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simsun.yasp.samples"
    >

  <application
      android:allowBackup="false"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      >
    <!--<meta-data android:name="firebase_performance_logcat_enabled" android:value="true" />-->
    <activity
        android:name="com.simsun.yasp.sample.MainActivity"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar"
        >
      <intent-filter android:label="@string/app_name">
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name="com.simsun.yasp.sample.SingleThreadActivity"> </activity>
    <activity android:name="com.simsun.yasp.sample.MultipleThreadActivity"> </activity>
    <activity android:name="com.simsun.yasp.sample.BenchmarkActivity"> </activity>
  </application>

</manifest>